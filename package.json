    "build:schematics": " ./node_modules/typescript/bin/tsc -p ./libs/schematics/tsconfig.json && cp ./libs/schematics/*.json ./dist/libs/rules/lib/schematics && cp ./libs/schematics/ng-add/*.json ./dist/libs/rules/lib/schematics/ng-add && cp -R ./libs/schematics/ng-add/files ./dist/libs/rules/lib/schematics/ng-add/files"

  },
