{
  "name": "ngx-metaui",
  "description": "Rule driven UI for Angular",
  "version": "7.0.5",
  "license": "Apache 2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "compile:oss:sys": "java -jar libs/rules/src/lib/resources/tools/oss/meta-ui-parser.jar --gen --sys ./libs/rules/src/lib/metaui/core && java -jar libs/rules/src/lib/resources/tools/oss/meta-ui-parser.jar --gen --sys ./libs/primeng-rules/src/lib/metaui && java -jar libs/rules/src/lib/resources/tools/oss/meta-ui-parser.jar --gen --sys ./libs/material-rules/src/lib/metaui",
    "compile:oss:evo": "java -jar libs/rules/src/lib/resources/tools/oss/meta-ui-parser.jar --gen --user ./libs/rules/src/lib/metaui/core ./apps/metaui-evolution/src/app/rules",
    "compile:oss:det": "java -jar libs/rules/src/lib/resources/tools/oss/meta-ui-parser.jar --gen --user ./libs/rules/src/lib/metaui/core ./apps/my-detail-page/src/app/rules",
    "compile:oss:md": "java -jar libs/rules/src/lib/resources/tools/oss/meta-ui-parser.jar --gen --user ./libs/rules/src/lib/metaui/core ./apps/material-app/src/app/rules",
    "copy:schematics": "cp ./libs/schematics/*.json ./dist/libs/rules/lib/schematics && cp ./libs/schematics/ng-add/*.json ./dist/libs/rules/lib/schematics/ng-add && cp -R ./libs/schematics/ng-add/files ./dist/libs/rules/lib/schematics/ng-add/files && cp ./libs/schematics/ng-generate/m-page/*.json ./dist/libs/rules/lib/schematics/ng-generate/m-page && cp -R ./libs/schematics/ng-generate/m-page/files ./dist/libs/rules/lib/schematics/ng-generate/m-page/files",
    "build:schematics": " ./node_modules/typescript/bin/tsc -p ./libs/schematics/tsconfig.json && npm run copy:schematics"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/router": "^7.0.2",
    "@nrwl/nx": "7.0.0",
    "big-integer": "1.6.36",
    "core-js": "^2.5.4",
    "flexboxgrid": "^6.3.1",
    "font-awesome": "4.7.0",
    "intl": "1.2.5",
    "object-hash": "1.3.0",
    "object-path": "^0.11.4",
    "primeicons": "^1.0.0-beta.10",
    "primeng": "7.0.0-beta.1",
    "quill": "1.3.6",
    "rxjs": "6.3.3",
    "typescript-collections": "1.3.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.4",
    "@angular-devkit/build-ng-packagr": "^0.10.4",
    "@angular-devkit/core": "^0.8.5",
    "@angular-devkit/schematics": "^0.8.5",
    "@angular/cli": "^7.0.4",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@nrwl/schematics": "7.0.0",
    "@schematics/angular": "^7.0.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/object-path": "^0.9.29",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "prettier": "1.13.7",
    "prismjs": "^1.15.0",
    "protractor": "~5.4.0",
    "scss-bundle": "^2.4.0",
    "standard-version": "^4.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "commit": true
    }
  }
}
